{
  "name": "mongoose-express-ts",
  "version": "1.0.0",
  "description": "Mongoose Node.js Express TypeScript application boilerplate with best practices for API development.",
  "author": "Paul Cham",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "watch-tsc": "tsc -w",
    "deploy": "node dist/src/server.js",
    "watch-deploy": "nodemon dist/src/server.js",
    "server": "concurrently \"npm run watch-tsc\" \"npm run watch-deploy\"",
    "start_": "npm run deploy"
  },
  "dependencies": {
    "aws-sdk": "^2.879.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.2.1",
    "express-validator": "^5.3.1",
    "gravatar": "^1.8.0",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.3",
    "node-isbn": "^1.6.1",
    "request": "^2.88.0",
    "serverless-http": "^2.7.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-typescript": "^1.1.9",
    "shortid": "^2.2.16",
    "typescript": "^3.4.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/gravatar": "^1.8.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.168",
    "@types/node-isbn": "^1.6.0",
    "@types/shortid": "^0.0.29",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0"
  }
